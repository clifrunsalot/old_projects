import java.io.*;
import javax.swing.ComboBoxModel;
import javax.swing.JComboBox;
import javax.swing.JList;
import javax.swing.event.ListDataListener;

public class DoubleComboBoxModel implements ComboBoxModel{

	public DoubleComboBoxModel(double s, double e){
		start = s;
		end = e;
		list = new JList();
		for(double i=start; i<end+1; i++){
			list.add(i);
		}
	}

	public Object getSelectedItem(){
		return item;
	}

	public void setSelectedItem(double d){
		item = d;
		addListDataListener(new ListDataListener());
	}

 	void addListDataListener(ListDataListener l){
		
 	}

	Object getElementAt(int index){
		return (Object)list.elementAt(index);
	}

	int getSize(){
		return list.size();
	}
	
	void removeListDataListener(ListDataListener l){

	}
			 
	private double start = 0.0;
	private double end = 0.0;
	private double item = 0.0;
	private JList list = null;
}
